<ExecutionModel name="HotRollBonus">
		<Outcomes>
			<Outcome type="WagerOutcome" name="BaseGame.Lines" className="com.igt.gle.game.data.wrapper.outcome.WagerOutcomeWrapper" />
			<Outcome type="PrizeOutcome" name="BaseGame.Lines" className="com.igt.gle.game.data.wrapper.outcome.PrizeOutcomeWrapper" />
			<Outcome type="PrizeOutcome" name="Dice.Prizes" className="com.igt.gle.game.data.wrapper.outcome.PrizeOutcomeWrapper" />
			<Outcome type="PrizeOutcome" name="Dice.Total" className="com.igt.gle.game.data.wrapper.outcome.PrizeOutcomeWrapper" />
            <Outcome type="PrizeOutcome" name="PrizeMultiplier" className="com.igt.gle.game.data.wrapper.outcome.PrizeOutcomeWrapper" />
            <Outcome type="PrizeOutcome" name="PrizeMultiplier.Total" className="com.igt.gle.game.data.wrapper.outcome.PrizeOutcomeWrapper" />
			<Outcome type="HighlightOutcome" name="BaseGame.Lines" className="com.igt.gle.game.data.wrapper.outcome.HighlightOutcomeWrapper" />
			<Outcome type="PopulationOutcome" name="BaseGame.Reels" className="com.igt.gle.game.data.wrapper.outcome.PopulationOutcomeWrapper" />
			<Outcome type="PopulationOutcome" name="Dice" className="com.igt.gle.game.data.wrapper.outcome.PopulationOutcomeWrapper" />
			<Outcome type="RandomResponseOutcome" className="com.igt.gle.game.data.wrapper.outcome.RandomResponseOutcomeWrapper">
			<Option>encrypted</Option>
			</Outcome>
			<Outcome type="FreeSpinOutcome" className="com.igt.gle.game.data.wrapper.outcome.FreeSpinOutcomeWrapper"/>
			<Outcome type="TriggerOutcome" name="FreeSpin" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper">
					<Option>hidden</Option>
			</Outcome>
			<Outcome type="WagerOutcome" name="BaseGame.Scatter" className="com.igt.gle.game.data.wrapper.outcome.WagerOutcomeWrapper" />
			<Outcome type="WagerOutcome" name="Game.Total" className="com.igt.gle.game.data.wrapper.outcome.WagerOutcomeWrapper">
			<!-- <Option>hidden</Option> -->
			</Outcome>
			<Outcome type="PrizeOutcome" name="Game.Total" className="com.igt.gle.game.data.wrapper.outcome.PrizeOutcomeWrapper" />
			<Outcome type="AwardCapOutcome" name="AwardCap" className="com.igt.gle.game.data.wrapper.outcome.AwardCapOutcomeWrapper" />
			<Outcome type="TriggerOutcome" name="DiceResults" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper" />
			<Outcome type="TriggerOutcome" name="Picker" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper" />
			<Outcome type="PatternOutcome" name="DiceResultsForClient" className="com.igt.gle.game.data.wrapper.outcome.PatternOutcomeWrapper">
			<Option>hidden</Option>
			</Outcome>
			<Outcome type="TriggerOutcome" name="DiceForClient1" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper">
			<Option>hidden</Option>
			</Outcome>
			<Outcome type="TriggerOutcome" name="DiceForClient2" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper">
			<Option>hidden</Option>
			</Outcome>
			<Outcome type="MultiplierOutcome" name="Picker" 
            	className="com.igt.gle.game.data.wrapper.outcome.MultiplierOutcomeWrapper"  >
				 <!-- <Option>hidden</Option> -->
			</Outcome>
			<Outcome type="TriggerOutcome" name="Roll" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper"/>
			<Outcome type="TriggerOutcome" name="AccumulateHotRollResults" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper"/>
			<Outcome type="TriggerOutcome" name="AccumulateDice" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper">		
			</Outcome>						
			<Outcome type="TriggerOutcome" name="AccumulateRoll" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper"/>
			<Outcome type="TriggerOutcome" name="SevenOnceRoll" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper"/>
			<Outcome type="TriggerOutcome" name="SevenOut" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper"/>
			<Outcome type="TriggerOutcome" name="AwardCap" className="com.igt.gle.game.data.wrapper.outcome.TriggerOutcomeWrapper">
				<Option>hidden</Option>
			</Outcome>
			<Outcome type="StageOutcome" className="com.igt.gle.game.data.wrapper.outcome.StageOutcomeWrapper">
				<Option>hidden</Option>
			</Outcome>
			<Outcome type="TransactionOutcome" className="com.igt.gle.game.data.wrapper.outcome.TransactionOutcomeWrapper">
				<Option>hidden</Option>
			</Outcome>
		</Outcomes>
		<Components>
			<Populators>
				<Populator type="HotRollBonus.Populator" className="com.igt.gle.game.component.populator.Populator">
					<InputMapping>
						<Input source="outcome" type="PopulationOutcome" name="Dice" />
						<Input source="outcome" type="RandomResponseOutcome" />
					</InputMapping>
					<StrategyMapping>
						<Strategy name="stripLength" className="com.igt.gle.game.component.populator.strategy.StripLengthStrategy" />
					</StrategyMapping>
				</Populator>
			</Populators>
			<Processors>
				<Processor type="HotRollBonus.OutcomeDeepCopyProcessor"
					className="com.igt.gle.game.component.processor.OutcomeDeepCopyProcessor">
					<InputMapping>
						<Input source="previousOutcome" type="WagerOutcome" name="BaseGame.Lines" />
						<Input source="previousOutcome" type="WagerOutcome" name="BaseGame.Scatter" />
						<Input source="previousOutcome" type="WagerOutcome" name="Game.Total" />
						<Input source="previousOutcome" type="PrizeOutcome" name="BaseGame.Lines" />
						<Input source="previousOutcome" type="HighlightOutcome"
							name="BaseGame.Lines" />
						<Input source="previousOutcome" type="PopulationOutcome"
							name="BaseGame.Reels" />
						<Input source="previousOutcome" type="TriggerOutcome"
							name="Picker" />
						<Input source="previousOutcome" type="MultiplierOutcome"
							name="Picker" />
						<Input source="outcome" type="WagerOutcome" name="BaseGame.Lines" />
						<Input source="outcome" type="WagerOutcome" name="BaseGame.Scatter" />
						<Input source="outcome" type="WagerOutcome" name="Game.Total" />
						<Input source="outcome" type="PrizeOutcome" name="BaseGame.Lines" />
						<Input source="outcome" type="HighlightOutcome" name="BaseGame.Lines" />
						<Input source="outcome" type="PopulationOutcome" name="BaseGame.Reels" />
						<Input source="outcome" type="TriggerOutcome" name="Picker" />
						<Input source="outcome" type="MultiplierOutcome" name="Picker" />
					</InputMapping>
				</Processor>
				<Processor type="HotRollBonus.FreeSpinProcessor" className="com.igt.gle.game.component.processor.FreeSpinProcessor">
					<InputMapping>
						<Input source="previousOutcome" type="FreeSpinOutcome"/>
						<Input source="outcome" type="TriggerOutcome" name="FreeSpin"/>
						<Input source="outcome" type="FreeSpinOutcome"/>
					</InputMapping>
					<StrategyMapping>
						<Strategy name="Additive" className="com.igt.gle.game.component.processor.strategy.freespinincrement.AdditiveStrategy"/>
						<Strategy name="ConstantDecrement" className="com.igt.gle.game.component.processor.strategy.freespindecrement.ConstantDecrementStrategy"/>
					</StrategyMapping>
				</Processor>
				<Processor type="HotRollBonus.PopulationTriggerProcessor" className="com.igt.gle.game.component.processor.PopulationTriggerProcessor">
					<InputMapping>
						<Input source="refOutcome" type="PopulationOutcome" name="Dice" />
						<Input source="outcome" type="TriggerOutcome" name="DiceResults" />
					</InputMapping>
					<StrategyMapping>
						<Strategy name="default" className="com.igt.gle.game.component.processor.strategy.populationtriggerprocessor.DefaultStrategy" />
					</StrategyMapping>
				</Processor>
				<Processor type="HotRollBonus.PatternProcessor" className="com.igt.gle.game.component.processor.PatternProcessor">
					<InputMapping>
						<Input source="refOutcome" type="PopulationOutcome" name="Dice" />
						<Input source="outcome" type="PatternOutcome" name="DiceResultsForClient" />
					</InputMapping>
					<StrategyMapping>
						<Strategy name="default" className="com.igt.gle.game.component.processor.strategy.patternprocessor.DefaultPatternProcessorStrategy" />
					</StrategyMapping>
				</Processor>
				<Processor type="L0C0R0.PatternCellTriggerProcessor" className="com.igt.gle.game.component.processor.PatternCellTriggerProcessor">
					<InputMapping>
						<Input source="refOutcome" type="PatternOutcome" name="DiceResultsForClient" />
						<Input source="outcome" type="TriggerOutcome" name="DiceForClient1" />
					</InputMapping>
				</Processor>
				<Processor type="L0C1R0.PatternCellTriggerProcessor" className="com.igt.gle.game.component.processor.PatternCellTriggerProcessor">
					<InputMapping>
						<Input source="refOutcome" type="PatternOutcome" name="DiceResultsForClient" />
						<Input source="outcome" type="TriggerOutcome" name="DiceForClient2" />
					</InputMapping>
				</Processor>
				<Processor type="TriggerProcessor" className="com.igt.gle.game.component.processor.TriggerProcessor">
					<InputMapping>
						<Input source="refOutcome" type="TriggerOutcome" name="DiceResults" />
						<Input source="outcome" type="TriggerOutcome" name="Roll" />
					</InputMapping>
				</Processor>
				<Processor type="AccumulateTriggerProcessor" className="com.igt.gle.game.component.processor.TriggerProcessor">
					<InputMapping>
						<Input source="refOutcome" type="TriggerOutcome" name="DiceResults" />
						<Input source="outcome" type="TriggerOutcome" name="AccumulateRoll" />
					</InputMapping>
				</Processor>
				
				<Processor type="SevenOnceRoll.TriggerProcessor" className="com.igt.gle.game.component.processor.TriggerProcessor">
					<InputMapping>
						<Input source="refOutcome" type="TriggerOutcome" name="DiceResults" />
						<Input source="refOutcome" type="TriggerOutcome" name="Picker" />
						<Input source="outcome" type="TriggerOutcome" name="SevenOnceRoll" />
					</InputMapping>
				</Processor>
				<Processor type="SevenOut.TriggerProcessor" className="com.igt.gle.game.component.processor.TriggerProcessor">
					<InputMapping>
						<Input source="refOutcome" type="TriggerOutcome" name="SevenOnceRoll" />
						<Input source="previousOutcome" type="TriggerOutcome" name="AccumulateDice" />
						<Input source="outcome" type="TriggerOutcome" name="SevenOut" />
					</InputMapping>
				</Processor>
				<Processor type="HotRoll.TriggerAggregateProcessor" className="com.igt.gle.game.component.processor.TriggerMergeProcessor">
					<InputMapping>
						<Input source="previousOutcome" type="TriggerOutcome" name="AccumulateDice" />
						<Input source="refOutcome" type="TriggerOutcome" name="AccumulateRoll" />
						<Input source="outcome" type="TriggerOutcome" name="AccumulateDice" />
					</InputMapping>
				</Processor>
				<Processor type="HotRoll.TriggerAggregateProcessorForClient" className="com.igt.gle.game.component.processor.TriggerMergeProcessor">
					<InputMapping>
						<Input source="previousOutcome" type="TriggerOutcome" name="AccumulateHotRollResults" />							
						<Input source="refOutcome" type="TriggerOutcome" name="DiceForClient1" />					
						<Input source="refOutcome" type="TriggerOutcome" name="DiceForClient2" />
						<Input source="outcome" type="TriggerOutcome" name="AccumulateHotRollResults" />
					</InputMapping>
				</Processor>
				
				<Processor type="HotRollBonus.PrizeProcessor" className="com.igt.gle.game.component.processor.PrizeProcessor">
					<InputMapping>
						<Input source="refOutcome" type="WagerOutcome" name="BaseGame.Scatter" />
						<Input source="refOutcome" type="TriggerOutcome" name="Roll" />
						<Input source="refOutcome" type="MultiplierOutcome" name="Picker" />
						<Input source="outcome" type="PrizeOutcome" name="Dice.Prizes" />
					</InputMapping>
					<StrategyMapping>
						<Strategy name="PayTrigger" className="com.igt.gle.game.component.processor.strategy.prize.PayTriggerStrategy" />
						<Strategy name="multiplyAll" className="com.igt.gle.game.component.processor.strategy.prizemultiplier.MultiplyAllStrategy" />
					</StrategyMapping>
				</Processor>
                <Processor type="HotRollBonus.ClientPrizeProcessor" className="com.igt.gle.game.component.processor.PrizeProcessor">
                    <InputMapping>
                        <Input source="refOutcome" type="TriggerOutcome" name="Roll" />
                        <Input source="outcome" type="PrizeOutcome" name="PrizeMultiplier" />
                    </InputMapping>
                    <StrategyMapping>
                        <Strategy name="PayTrigger" className="com.igt.gle.game.component.processor.strategy.prize.PayTriggerStrategy" />
                        <Strategy name="multiplyAll" className="com.igt.gle.game.component.processor.strategy.prizemultiplier.MultiplyAllStrategy" />
                    </StrategyMapping>
                </Processor>
				<Processor type="HotRollBonus.PrizeAggregateProcessor" className="com.igt.gle.game.component.processor.PrizeAggregateProcessor">
					<InputMapping>
                        <Input source="previousOutcome" type="PrizeOutcome" name="Dice.Total" />
						<Input source="refOutcome" type="PrizeOutcome" name="Dice.Prizes" />
						<Input source="outcome" type="PrizeOutcome" name="Dice.Total" />
					</InputMapping>
				</Processor>
                <Processor type="HotRollBonus.ClientPrizeAggregateProcessor" className="com.igt.gle.game.component.processor.PrizeAggregateProcessor">
                    <InputMapping>
                        <Input source="previousOutcome" type="PrizeOutcome" name="PrizeMultiplier.Total" />
                        <Input source="refOutcome" type="PrizeOutcome" name="PrizeMultiplier" />
                        <Input source="outcome" type="PrizeOutcome" name="PrizeMultiplier.Total" />
                    </InputMapping>
                </Processor>
				<Processor type="Game.PrizeAggregateProcessor" className="com.igt.gle.game.component.processor.PrizeAggregateProcessor">
					<InputMapping>
						<Input source="refOutcome" type="PrizeOutcome" name="BaseGame.Lines" />
						<Input source="refOutcome" type="PrizeOutcome" name="Dice.Total" />
						<Input source="outcome" type="PrizeOutcome" name="Game.Total" />
					</InputMapping>
				</Processor>
				<Processor type="Game.AwardCapProcessor" className="com.igt.gle.game.component.processor.AwardCapProcessor">
					<InputMapping>
						<Input source="outcome" type="PrizeOutcome" name="Game.Total" />
						<Input source="outcome" type="WagerOutcome" name="Game.Total" />
						<Input source="outcome" type="TriggerOutcome" name="AwardCap" />
						<Input source="outcome" type="AwardCapOutcome" name="AwardCap" />
					</InputMapping>
				</Processor>
				<Processor type="HotRollBonus.StageProcessor" className="com.igt.gle.game.component.processor.StageProcessor">
					<InputMapping>
						<Input source="refOutcome" type="TriggerOutcome" name="SevenOut" />
						<Input source="refOutcome" type="TriggerOutcome" name="AwardCap" />
						<Input source="refOutcome" type="TriggerOutcome" name="FreeSpin" />
						<Input source="outcome" type="StageOutcome" />
					</InputMapping>
				</Processor>
				<Processor type="HotRollBonus.AdvancedTransactionProcessor" className="com.igt.gle.game.component.processor.AdvancedTransactionProcessor">
					<InputMapping>
                        <Input source="refOutcome" type="TriggerOutcome" name="SevenOut" />
						<Input source="refOutcome" type="TriggerOutcome" name="AwardCap" />
						<Input source="refOutcome" type="PrizeOutcome" name="Game.Total" />
						<Input source="refOutcome" type="TriggerOutcome" name="FreeSpin" />
						<Input source="outcome" type="WagerOutcome" name="Game.Total" />
						<Input source="outcome" type="TransactionOutcome" />
					</InputMapping>
				</Processor>
			</Processors>
		</Components>
	</ExecutionModel>